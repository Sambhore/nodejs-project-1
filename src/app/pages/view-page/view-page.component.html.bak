<app-header [pageName]="pageName"></app-header>
<div class="container-fluid p-5">
    <div class="row application-list mb-5">
        <div class="col-12">
            <div class="accordion" id="accordionPanelsStayOpen">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="panelsStayOpen-headingOne">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                            data-bs-target="#panelsStayOpen-collapseOne" aria-expanded="false"
                            aria-controls="panelsStayOpen-collapseOne">
                            <b>Applicants : {{applicants.length}}</b>
                            <img class="float-end arrow" src="./assets/images/icons/Icon ionic-ios-arrow-dropright-circle.svg">
                        </button>
                    </h2>
                    <div id="panelsStayOpen-collapseOne" class="accordion-collapse collapse overflow-auto"
                        aria-labelledby="panelsStayOpen-headingOne">
                        <div class="accordion-body d-table" #applicantsElm>
                            <div class="d-table-row">
                                <div class="card applicant-card d-table-cell" (click)="getApplicantDetails($event, applicant)" *ngFor="let applicant of applicants;let i = index">
                                    <div class="card-body">
                                        <h4>Application {{i + 1}}</h4>
                                        <div class="row">
                                            <div class="col">Name</div>
                                            <div class="col text-end fw-bold">{{applicant.custfname}}</div>
                                        </div>
                                        <div class="row">
                                            <div class="col">Category</div>
                                            <div class="col text-end fw-bold">{{applicant.fincategory}}</div>
                                        </div>
                                        <div class="row">
                                            <div class="col">Segment</div>
                                            <div class="col text-end fw-bold">{{applicant.custsegment}}</div>
                                        </div>
                                        <div class="row">
                                            <div class="col">Loan Amount</div>
                                            <div class="col text-end fw-bold">{{applicant.finamount}}</div>
                                        </div>
                                        <div class="row">
                                            <div class="col">Product</div>
                                            <div class="col text-end fw-bold">{{applicant.fintype}}</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row application-details mb-5">
        <div class="col-12">
            <div class="accordion" id="accordionPanelsStayOpen">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="panelsStayOpen-headingTwo">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                            data-bs-target="#panelsStayOpen-collapseTwo" aria-expanded="false"
                            aria-controls="panelsStayOpen-collapseTwo">
                            <b>Triggered Services</b>
                            <img class="float-end arrow" src="./assets/images/icons/Icon ionic-ios-arrow-dropright-circle.svg">
                        </button>
                    </h2>
                    <div id="panelsStayOpen-collapseTwo" class="accordion-collapse collapse "
                        aria-labelledby="panelsStayOpen-headingTwo">
                        <div class="accordion-body">
                            <div class="service-wrapper row row-cols-5">
                                <div class="col service-block border-end align-self-start" *ngFor="let service of applicantDetails;">
                                    <div class="service">
                                        <h4>{{service.key}}</h4>
                                        <div class="service-item row g-0" *ngFor="let detail of service.values;">
                                            <div class="col-auto align-self-start">
                                                <span class="circle" [ngClass]="{'tick': detail.status == 'Ready' || detail.status == 'PENDING', 'deactive': detail.status === 'deactive'}">âœ”</span>
                                            </div>
                                            <div class="col-auto doc-name">
                                                <b>{{detail.name}}</b><br>
                                                <span>{{detail.date}}</span>
                                            </div>
                                            <div class="col status">
                                                <span class="badge rounded-pill text-bg-{{detail.status}} float-end">{{detail.status}}</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="hstack gap-5 services-available">
        <div class="align-self-start col-5" #setvicesAvailable>
            <div class="card">
                <div class="card-body">
                    <div class="header">
                        <h1>Services Available</h1>
                        <button class="expand-btn float-end" (click)="expandAll($event, 'setvicesAvailable')">Expand All</button>
                        <p>
                            Please select all the API checks required for this case from below list
                        </p>
                    </div>
                    <div class="container-fluid">
                        <div class="row row-cols-2">
                            <div class="col mb-4" *ngFor="let service of services;">
                                <div class="accordion" id="accordionPanelsStayOpen">
                                    <div class="accordion-item">
                                        <h2 class="accordion-header" id="panelsStayOpen-heading{{service.key | removeSpaces}}">
                                            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                                attr.data-bs-target="#panelsStayOpen-collapse{{service.key | removeSpaces}}" aria-expanded="false"
                                                attr.aria-controls="panelsStayOpen-collapse{{service.key | removeSpaces}}">
                                                {{service.key}}
                                                <img class="float-end arrow" src="./assets/images/icons/Icon ionic-ios-arrow-dropright-circle.svg">
                                            </button>
                                        </h2>
                                        <div attr.id="panelsStayOpen-collapse{{service.key | removeSpaces}}" class="accordion-collapse collapse "
                                            attr.aria-labelledby="panelsStayOpen-heading{{service.key | removeSpaces}}">
                                            <div class="accordion-body">
                                                <div class="form-check" *ngFor="let check of service.values;">
                                                    <input class="form-check-input" type="checkbox" (change)="onServiceSelect($event)" value="{{check.id}}" id="{{check.id}}" [checked]="check.status == 'ready' || check.status == 'PENDING'" [disabled]="apiDetails[check.id].disabled">
                                                    <label class="form-check-label" for="{{check.id}}">
                                                        {{check.name}}
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="">
            <div class="d-flex" style="height: 500px;">
                <div class="vr"></div>
            </div>
        </div>    
        <div class="align-self-start col-5 submit-to-api" #SubmitDocuments>
            <div class="card">
                <div class="card-body">
                    
                    <div class="header clearfix mb-3">
                        <button class="expand-btn float-end submit-btn" [ngClass]="{'active' : submitButton}" [disabled]="!submitButton" (click)="submitForm()">Submit for API</button>
                        <h1>Submit Documents</h1>
                        <p>
                            Please add all required fields below to process services,
                        </p>
                        <button class="expand-btn float-end" (click)="expandAll($event, 'SubmitDocuments')">Expand All</button>
                    </div>
                    
                    <div class="row mb-4" *ngIf="apiDetails['1'].checked">
                        <div class="col-12">
                            <div class="accordion" id="accordionPanelsStayOpen" >
                                <div class="accordion-item">
                                    <h2 class="accordion-header" id="panelsStayOpen-headingPanCard">
                                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                            data-bs-target="#panelsStayOpen-collapsePanCard" aria-expanded="false"
                                            aria-controls="panelsStayOpen-collapsePanCard">
                                            Pan Card
                                            <img class="float-end arrow" src="./assets/images/icons/Icon ionic-ios-arrow-dropright-circle.svg">
                                        </button>
                                    </h2>
                                    <div id="panelsStayOpen-collapsePanCard" class="accordion-collapse collapse "
                                        aria-labelledby="panelsStayOpen-headingPanCard">
                                        <div class="accordion-body file-upload">
                                            <form [formGroup]="pan_card_form">
                                                <div class="row">
                                                    <div class="col">
                                                        <div class="alert alert-danger" role="alert" *ngIf="apiDetails['1'].error">
                                                            {{apiDetails['1'].error_message}}
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row align-items-center mb-4">
                                                    <div class="col-6">
                                                        <p class="mb-0">Pan Number<span class="float-end">(required)</span></p>
                                                        <input type="input" formControlName="pan" class="form-control"/>
                                                    </div>
                                                    <div class="col-6">
                                                        <p class="mb-0">Name<span class="float-end">(required)</span></p>
                                                        <input type="input" formControlName="name" class="form-control"/>
                                                    </div>
                                                </div>
                                                <div class="row align-items-center mb-4">
                                                    <div class="col-6 ">
                                                        <p class="mb-0">Date of Birth<span class="float-end">(required)</span></p>
                                                        <input type="date" formControlName="dob" class="form-control"/>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row mb-4" *ngIf="apiDetails['24'].checked">
                        <div class="col-12">
                            <div class="accordion" id="accordionPanelsStayOpen" >
                                <div class="accordion-item">
                                    <h2 class="accordion-header" id="panelsStayOpen-headingAdharxml">
                                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                            data-bs-target="#panelsStayOpen-collapseAdharxml" aria-expanded="false"
                                            aria-controls="panelsStayOpen-collapseAdharxml">
                                            Adhar XML
                                            <img class="float-end arrow" src="./assets/images/icons/Icon ionic-ios-arrow-dropright-circle.svg">
                                        </button>
                                    </h2>
                                    <div id="panelsStayOpen-collapseAdharxml" class="accordion-collapse collapse "
                                        aria-labelledby="panelsStayOpen-headingAdharxml">
                                        <div class="accordion-body file-upload">
                                            <form [formGroup]="aadhar_xml_form">
                                                <div class="row">
                                                    <div class="col">
                                                        <div class="alert alert-danger" role="alert" *ngIf="apiDetails['24'].error">
                                                            {{apiDetails['24'].error_message}}
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row align-items-center mb-4">
                                                    <div class="col-6">
                                                        <p class="mb-0">File 1 <span class="float-end">(Required)</span></p>
                                                        <input type="file" id="file1" hidden (change)="onFileChange($event, 'aadhar_xml_form')" />
                                                        <label for="file1" class="text-center"><img src="./assets/images/icons/Icon ionic-ios-cloud-upload.svg"> Click to Select File</label>
                                                    </div>
                                                    <div class="col-6 ">
                                                        <div class="uploaded-file">
                                                            <p>
                                                                <img src="./assets/images/icons/Icon material-attach-file.svg">
                                                                Uploaded File
                                                                <img src="./assets/images/icons/Icon ionic-ios-close.svg" class="float-end">
                                                            </p>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row align-items-center mb-4">
                                                    <div class="col-6">
                                                        <p class="mb-0">Image <span class="float-end">(Required)</span></p>
                                                        <input type="file" id="image" hidden (change)="onFileChange($event, 'aadhar_xml_form')" />
                                                        <label for="image" class="text-center"><img src="./assets/images/icons/Icon material-attach-file.svg"> Click to Select File</label>
                                                    </div>
                                                    <div class="col-6 ">
                                                        <div class="uploaded-file">
                                                            <p>
                                                                <img src="./assets/images/icons/Icon material-attach-file.svg">
                                                                Uploaded File
                                                                <img src="./assets/images/icons/Icon ionic-ios-close.svg" class="float-end">
                                                            </p>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row align-items-center mb-4">
                                                    <div class="col-6">
                                                        <p class="mb-0">Share Code<span></span></p>
                                                        <input type="input" formControlName="shareCode" class="form-control"/>
                                                    </div>
                                                    <div class="col-6 ">
                                                        <p class="mb-0">Date of Birth<span></span></p>
                                                        <input type="input" formControlName="dob" class="form-control"/>
                                                    </div>
                                                </div>
                                                <div class="row align-items-center mb-4">
                                                    <div class="col-6">
                                                        <p class="mb-0">Name<span></span></p>
                                                        <input type="input" formControlName="name" class="form-control"/>
                                                    </div>
                                                    <div class="col-6 ">
                                                        <p class="mb-0">Address<span></span></p>
                                                        <input type="input" formControlName="address" class="form-control"/>
                                                    </div>
                                                </div>
                                                <div class="row align-items-center mb-4">
                                                    <div class="col-6">
                                                        <p class="mb-0">Mobile<span></span></p>
                                                        <input type="input" formControlName="mobile" class="form-control"/>
                                                    </div>
                                                    <div class="col-6 ">
                                                        <p class="mb-0">email<span></span></p>
                                                        <input type="input" formControlName="email" class="form-control"/>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row mb-4" *ngIf="apiDetails['3'].checked">
                        <div class="col-12">
                            <div class="accordion" id="accordionPanelsStayOpen" >
                                <div class="accordion-item">
                                    <h2 class="accordion-header" id="panelsStayOpen-headingPassport">
