                                                        <p class="mb-0">Extended Period<span></span></p>
                                                        <div class="toggle-button-cover">
                                                            <div class="button-cover">
                                                                <div class=" b2" id="button-10">
                                                                    <input type="checkbox" id="chbox1" formControlName="extendedPeriod" class="checkbox">
                                                                    <div class="knobs">
                                                                        <label for="chbox1">Yes</label>
                                                                    </div>
                                                                    <div class="layer"></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-6  mb-4" >
                                                        <p class="mb-0">Simulate Enable<span></span></p>
                                                        <div class="toggle-button-cover">
                                                            <div class="button-cover">
                                                                <div class=" b2" id="button-10">
                                                                    <input type="checkbox" id="chbox2" formControlName="simulateEnable" class="checkbox">
                                                                    <div class="knobs">
                                                                        <label for="chbox2">Yes</label>
                                                                    </div>
                                                                    <div class="layer"></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-6  mb-4" >
                                                        <p class="mb-0">GST Cert Flag<span></span></p>
                                                        <div class="toggle-button-cover">
                                                            <div class="button-cover">
                                                                <div class=" b2" id="button-10">
                                                                    <input type="checkbox" id="chbox3" formControlName="gstCertFlag" class="checkbox">
                                                                    <div class="knobs">
                                                                        <label for="chbox3">Yes</label>
                                                                    </div>
                                                                    <div class="layer"></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-6  mb-4" >
                                                        <p class="mb-0">Consent<span></span></p>
                                                        <div class="toggle-button-cover">
                                                            <div class="button-cover">
                                                                <div class=" b2" id="button-10">
                                                                    <input type="checkbox" id="chbox3" formControlName="consent" class="checkbox">
                                                                    <div class="knobs">
                                                                        <label for="chbox3">Yes</label>
                                                                    </div>
                                                                    <div class="layer"></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                    <div class="row mb-4" *ngIf="apiDetails['26'].checked">
                        <div class="col-12">
                            <div class="accordion" id="accordionPanelsStayOpen" >
                                <div class="accordion-item">
                                    <h2 class="accordion-header" id="panelsStayOpen-headingBSAONLINE">
                                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                            data-bs-target="#panelsStayOpen-collapseBSAONLINE" aria-expanded="false"
                                            aria-controls="panelsStayOpen-collapseBSAONLINE">
                                            BSA ONLINE
                                            <img class="float-end arrow" src="./assets/images/icons/Icon ionic-ios-arrow-dropright-circle.svg">
                                        </button>
                                    </h2>
                                    <div id="panelsStayOpen-collapseBSAONLINE" class="accordion-collapse collapse "
                                        aria-labelledby="panelsStayOpen-headingBSAONLINE">
                                        <div class="accordion-body file-upload">
                                            <form [formGroup]="bsa_online_form">
                                                <div class="row">
                                                    <div class="col">
                                                        <div class="alert alert-danger" role="alert" *ngIf="apiDetails['26'].error">
                                                            {{apiDetails['26'].error_message}}
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row row-cols-2 align-items-center">
                                                    <div class="col-6  mb-4">
                                                        <p class="mb-0">Loan Type<span></span></p>
                                                        <input type="input" formControlName="loanType" class="form-control"/>
                                                    </div>
                                                    <div class="col-6  mb-4">
                                                        <p class="mb-0">Employment Type<span></span></p>
                                                        <select class="form-select" formControlName="employmentType" aria-label="Default select example">
                                                            <option selected value="Salaried">SALARIED</option>
                                                            <option selected value="SelfEmployed">SELF EMPLOYED</option>
                                                        </select>
                                                    </div>
                                                    <div class="col-6  mb-4" >
                                                        <p class="mb-0">Destination<span></span></p>
                                                        <select class="form-select" formControlName="destination" aria-label="Default select example">
                                                            <option value="netbankingFetch">Netbanking Fetch</option>
                                                            <option value="statement">Statement</option>
                                                            <option value="choice">Choice</option>
                                                            <option value="choice-all">Choice All</option>
                                                            <option value="nbf-all">NBF All</option>
                                                            <option value="statement-all">Statement All</option>
                                                            <option value="accountAggregator">Account Aggregator</option>
                                                        </select>
                                                    </div>
                                                    <div class="col-6  mb-4">
                                                        <p class="mb-0">Loan Duration<span></span></p>
                                                        <select class="form-select" formControlName="loanDuration" aria-label="Default select example">
                                                            <option *ngFor="let mon of months" value="{{mon}}" >{{mon}}</option>
                                                        </select>
                                                    </div>
                                                    <div class="col-6  mb-4">
                                                        <p class="mb-0">Loan Amount<span></span></p>
                                                        <input type="input" formControlName="loanAmount" class="form-control"/>
                                                    </div>
                                                    <div class="col-6  mb-4" >
                                                        <p class="mb-0">Acceptance Policy<span></span></p>
                                                        <select class="form-select" formControlName="acceptancePolicy" aria-label="Default select example">
                                                            <option value="atLeastOneTransactionInRange">At Least One Transaction In Range</option>
                                                            <option value="atLeastOneTransactionInStartAndEndMonths">At Least One Transaction In Start And End Months</option>
                                                            <option value="atLeastOneTransactionPerMonthInRange">At Least One Transaction Per Month In Range</option>
                                                        </select>
                                                    </div>
                                                    <div class="col-6  mb-4">
                                                        <p class="mb-0">Year Month From<span></span></p>
                                                        <input type="month" formControlName="yearMonthFrom" class="form-control"/>
                                                    </div>
                                                    <div class="col-6  mb-4">
                                                        <p class="mb-0">Year Month To<span></span></p>
                                                        <input type="month" formControlName="yearMonthTo" class="form-control"/>
                                                    </div>
                                                    <div class="col-6  mb-4" >
                                                        <p class="mb-0">facility<span></span></p>
                                                        <select class="form-select" formControlName="facility" aria-label="Default select example">
                                                            <option value="NONE">NONE</option>
                                                            <option value="OD">OD</option>
                                                            <option value="CC">CC</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>

<div id="backdrop" [ngClass]="{'d-none': !loader}" #backdrop>
    <div class="div-center loading">
        <div class="spinner-border" role="status" style="width: 5rem; height: 5rem;">
            <span class="sr-only"></span>
        </div>
    </div>
</div>


<div id="backdrop" [ngClass]="{'d-none': !apimessage}" #backdrop>
    <div class="div-center">
        <div class="api-message">
            <h4>Form Submit Status</h4>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Form Name</th>
                        <th scope="col">Status</th>
                        <th scope="col">Message</th>
                    </tr>
                </thead>
                <tbody>
                    <tr *ngFor="let status of apiStatus">
                        <td>{{status.form}}</td>
                        <td>{{status.status}}</td>
                        <td>{{status.message}}</td>
                    </tr>
                </tbody>
            </table>
            <div >
                <button class="btn btn-primary float-end" (click)="closePupup()">Ok</button>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>

